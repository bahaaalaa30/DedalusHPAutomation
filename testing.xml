<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Dedalus HP Automation Suite" parallel="classes" thread-count="2">

    <listeners>
    </listeners>

    <test name="Regression - Patient Management" preserve-order="true">
        <groups>
            <run>
                <include name="Regression"/>
            </run>
        </groups>

        <classes>
            <class name="Normalcases.LoginTest"/>
            <class name="Normalcases.SearchPatient"/>
            <class name="Normalcases.VisitBooking"/>
            <class name="Normalcases.ChangeLanguage"/>
            <class name="Normalcases.Cancelvisit"/>
            <class name="Normalcases.Cancelappointment"/>
        </classes>
    </test>
</suite>